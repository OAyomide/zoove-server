datasource postgresql {
  url      = env("DB_URL")
  provider = "postgresql"
}

generator db {
  provider      = "go run github.com/prisma/prisma-client-go"
  binaryTargets = ["native"]
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime
  fullName  String
  firstName String
  lastName  String
  country   String
  lang      String
  uuid      String   @unique
  email     String   @unique
}
