datasource postgresql {
  url = "***"
  provider = "postgresql"
}

generator db {
  provider      = "go run github.com/prisma/prisma-client-go"
  binaryTargets = ["native"]
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime
  fullName  String
  firstName String
  LastName  String
  Country   String
  Lang      String
}
